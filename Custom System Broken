local AntiKick_Toggle = Instance.new("TextButton")
AntiKick_Toggle.Name = "AntiKick_Toggle"
AntiKick_Toggle.Parent = AntiKick_Frame
AntiKick_Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
AntiKick_Toggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
AntiKick_Toggle.BorderSizePixel = 1
AntiKick_Toggle.Position = UDim2.new(0.8, 0, 0, 0)
AntiKick_Toggle.Size = UDim2.new(0, 25, 0, 25)
AntiKick_Toggle.Font = Enum.Font.Gotham
AntiKick_Toggle.Text = ""
AntiKick_Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
AntiKick_Toggle.TextSize = 14.000

local antiKickEnabled = false

AntiKick_Toggle.MouseButton1Click:Connect(function()
    antiKickEnabled = not antiKickEnabled
    AntiKick_Toggle.BackgroundColor3 = antiKickEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    
    if antiKickEnabled then
        -- Set up anti-kick (basic implementation - may not work in all games)
        local mt = getrawmetatable(game)
        local oldNamecall = mt.__namecall
        setreadonly(mt, false)
        
        mt.__namecall = newcclosure(function(self, ...)
            local method = getnamecallmethod()
            local args = {...}
            
            if method == "Kick" and self == game.Players.LocalPlayer then
                return
            end
            
            return oldNamecall(self, ...)
        end)
        
        setreadonly(mt, true)
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "Anti Kick",
            Text = "Anti Kick has been enabled",
            Duration = 3
        })
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = "Anti Kick",
            Text = "Anti Kick has been disabled (requires script restart to take effect)",
            Duration = 3
        })
    end
end)

-- Toggle for ESP
local ESP_Frame = Instance.new("Frame")
ESP_Frame.Name = "ESP_Frame"
ESP_Frame.Parent = Settings_Frame
ESP_Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ESP_Frame.BackgroundTransparency = 0.8
ESP_Frame.BorderColor3 = Color3.fromRGB(255, 255, 255)
ESP_Frame.BorderSizePixel = 0
ESP_Frame.Position = UDim2.new(0, 5, 0, 130)
ESP_Frame.Size = UDim2.new(1, -10, 0, 25)

local ESP_Label = Instance.new("TextLabel")
ESP_Label.Name = "ESP_Label"
ESP_Label.Parent = ESP_Frame
ESP_Label.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ESP_Label.BackgroundTransparency = 1.000
ESP_Label.BorderColor3 = Color3.fromRGB(0, 0, 0)
ESP_Label.BorderSizePixel = 0
ESP_Label.Position = UDim2.new(0, 0, 0, 0)
ESP_Label.Size = UDim2.new(0.7, 0, 1, 0)
