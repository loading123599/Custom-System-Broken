-- Create theme options
for themeName, _ in pairs(themes) do
    local option = Instance.new("TextButton")
    option.Name = themeName .. "_Option"
    option.Parent = Theme_Options
    option.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    option.BorderColor3 = Color3.fromRGB(255, 255, 255)
    option.BorderSizePixel = 0
    option.Size = UDim2.new(1, 0, 0, 25)
    option.Font = Enum.Font.Gotham
    option.Text = themeName
    option.TextColor3 = Color3.fromRGB(255, 255, 255)
    option.TextSize = 14.000
    option.ZIndex = 10
    
    option.MouseButton1Click:Connect(function()
        Theme_Dropdown.Text = themeName
        Theme_Options.Visible = false
        applyTheme(themeName)
    end)
end

Theme_Dropdown.MouseButton1Click:Connect(function()
    Theme_Options.Visible = not Theme_Options.Visible
end)

-- Apply default theme
applyTheme("Default")

-- Clean up when the script is terminated
game.Players.LocalPlayer.CharacterRemoving:Connect(function()
    cleanup()
end)

-- Final setup
Background.Visible = true

-- Save settings function
local function saveSettings()
    local settings = {
        autoRejoin = autoRejoinEnabled,
        autoRespawn = autoRespawnEnabled,
        antiKick = antiKickEnabled,
        esp = espEnabled,
        theme = _G.CurrentTheme or "Default",
        keybinds = Keybinds
    }
    
    -- Convert settings to JSON
    local settingsJSON = game:GetService("HttpService"):JSONEncode(settings)
    
    -- Save to local storage
    pcall(function()
        writefile("PoisonsHub_Settings.json", settingsJSON)
    end)
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "Settings Saved",
        Text = "Your settings have been saved",
        Duration = 3
    })
end

-- Load settings function
local function loadSettings()
    pcall(function()
        if isfile("PoisonsHub_Settings.json") then
            local settingsJSON = readfile("PoisonsHub_Settings.json")
            local settings = game:GetService("HttpService"):JSONDecode(settingsJSON)
            
            -- Apply loaded settings
            if settings.autoRejoin ~= nil then
                autoRejoinEnabled = settings.autoRejoin
                AutoRejoin_Toggle.BackgroundColor3 = autoRejoinEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
                
                if autoRejoinEnabled then
                    -- Set up auto rejoin
                    local autoRejoinConnection
                    autoRejoinConnection = game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
                        if child.Name == "ErrorPrompt" and child:FindFirstChild("MessageArea") and child.MessageArea:FindFirstChild("ErrorFrame") then
                            game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
                        end
                    end)
                    
                    -- Store the connection for later cleanup
                    _G.AutoRejoinConnection = autoRejoinConnection
                end
            end
            
            if settings.autoRespawn ~= nil then
                autoRespawnEnabled = settings.autoRespawn
                AutoRespawn_Toggle.BackgroundColor3 = autoRespawnEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
                
                if autoRespawnEnabled then
                    -- Set up auto respawn
                    local autoRespawnConnection
                    autoRespawnConnection = game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
                        character:WaitForChild("Humanoid").Died:Connect(function()
                            task.wait(1) -- Wait a second before respawning
                            game.Players.LocalPlayer.Character:BreakJoints()
                        end)
                    end)
                    
                    -- If the player already has a character, set up the connection for it too
                    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                        game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
                            task.wait(1)
                            game.Players.LocalPlayer.Character:BreakJoints()
                        end)
                    end
                    
                    -- Store the connection for later cleanup
                    _G.AutoRespawnConnection = autoRespawnConnection
                end
            end
            
            if settings.antiKick ~= nil then
                antiKickEnabled = settings.antiKick
                AntiKick_Toggle.BackgroundColor3 = antiKickEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
                
                if antiKickEnabled then
                    -- Set up anti-kick
                    local mt = getrawmetatable(game)
                    local oldNamecall = mt.__namecall
                    setreadonly(mt, false)
                    
                    mt.__namecall = newcclosure(function(self, ...)
                        local method = getnamecallmethod()
                        local args = {...}
                        
                        if method == "Kick" and self == game.Players.LocalPlayer then
                            return
                        end
                        
                        return oldNamecall(self, ...)
                    end)
                    
                    setreadonly(mt, true)
                end
            end
            
            if settings.esp ~= nil then
                espEnabled = settings.esp
                ESP_Toggle.BackgroundColor3 = espEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
                
                if espEnabled then
                    enableESP()
                else
                    disableESP()
                end
            end
            
            if settings.theme then
                Theme_Dropdown.Text = settings.theme
                applyTheme(settings.theme)
            end
            
            if settings.keybinds then
                Keybinds = settings.keybinds
            end
            
            game.StarterGui:SetCore("SendNotification", {
                Title = "Settings Loaded",
                Text = "Your settings have been loaded",
                Duration = 3
            })
        end
    end)
end

