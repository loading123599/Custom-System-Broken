local AutoRespawn_Toggle = Instance.new("TextButton")
AutoRespawn_Toggle.Name = "AutoRespawn_Toggle"
AutoRespawn_Toggle.Parent = AutoRespawn_Frame
AutoRespawn_Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
AutoRespawn_Toggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
AutoRespawn_Toggle.BorderSizePixel = 1
AutoRespawn_Toggle.Position = UDim2.new(0.8, 0, 0, 0)
AutoRespawn_Toggle.Size = UDim2.new(0, 25, 0, 25)
AutoRespawn_Toggle.Font = Enum.Font.Gotham
AutoRespawn_Toggle.Text = ""
AutoRespawn_Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoRespawn_Toggle.TextSize = 14.000

local autoRespawnEnabled = false

AutoRespawn_Toggle.MouseButton1Click:Connect(function()
    autoRespawnEnabled = not autoRespawnEnabled
    AutoRespawn_Toggle.BackgroundColor3 = autoRespawnEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    
    if autoRespawnEnabled then
        -- Set up auto respawn
        local autoRespawnConnection
        autoRespawnConnection = game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
            character:WaitForChild("Humanoid").Died:Connect(function()
                task.wait(1) -- Wait a second before respawning
                game.Players.LocalPlayer.Character:BreakJoints()
            end)
        end)
        
        -- If the player already has a character, set up the connection for it too
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
                task.wait(1)
                game.Players.LocalPlayer.Character:BreakJoints()
            end)
        end
        
        -- Store the connection for later cleanup
        _G.AutoRespawnConnection = autoRespawnConnection
    else
        -- Clean up auto respawn
        if _G.AutoRespawnConnection then
            _G.AutoRespawnConnection:Disconnect()
            _G.AutoRespawnConnection = nil
        end
    end
end)

