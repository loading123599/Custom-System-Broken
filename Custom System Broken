-- Add a minimize button
local Minimize_Button = Instance.new("TextButton")
Minimize_Button.Name = "Minimize_Button"
Minimize_Button.Parent = Background
Minimize_Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Minimize_Button.BackgroundTransparency = 0.500
Minimize_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
Minimize_Button.BorderSizePixel = 0
Minimize_Button.Position = UDim2.new(1, -60, 0, 5)
Minimize_Button.Size = UDim2.new(0, 25, 0, 25)
Minimize_Button.Font = Enum.Font.Oswald
Minimize_Button.Text = "-"
Minimize_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
Minimize_Button.TextScaled = true
Minimize_Button.TextSize = 14.000
Minimize_Button.TextWrapped = true

local minimized = false
local originalSize = Background.Size
local originalPosition = Background.Position
local minimizedSize = UDim2.new(0, 200, 0, 30)
local minimizedPosition = UDim2.new(0.5, -100, 0, 0)

Minimize_Button.MouseButton1Click:Connect(function()
    minimized = not minimized
    
    if minimized then
        -- Save current size and position
        originalSize = Background.Size
        originalPosition = Background.Position
        
        -- Minimize
        for _, obj in pairs(Background:GetChildren()) do
            if obj ~= TopBar and obj ~= Minimize_Button and obj ~= Title_Label then
                obj.Visible = false
            end
        end
        
        Background.Size = minimizedSize
        Background.Position = minimizedPosition
        Minimize_Button.Text = "+"
    else
        -- Restore
        Background.Size = originalSize
        Background.Position = originalPosition
        
        for _, obj in pairs(Background:GetChildren()) do
            if obj:IsA("Frame") or obj:IsA("ScrollingFrame") then
                if obj.Name:find("Section") then
                    -- Only make the active section visible
                    if obj.Name == "Universal_Section" then
                        obj.Visible = true
                    end
                else
                    obj.Visible = false
                end
            elseif obj:IsA("TextButton") and obj ~= Minimize_Button then
                obj.Visible = true
            elseif obj:IsA("TextLabel") and obj ~= Title_Label then
                obj.Visible = true
            end
        end
        
        -- Make sure tab buttons are visible
        Universal_Button.Visible = true
        Target_Button.Visible = true
        Animations_Button.Visible = true
        Misc_Button.Visible = true
        Credits_Button.Visible = true
        
        Minimize_Button.Text = "-"
    end
end)

