local autoRejoinEnabled = false

AutoRejoin_Toggle.MouseButton1Click:Connect(function()
    autoRejoinEnabled = not autoRejoinEnabled
    AutoRejoin_Toggle.BackgroundColor3 = autoRejoinEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    
    if autoRejoinEnabled then
        -- Set up auto rejoin
        local autoRejoinConnection
        autoRejoinConnection = game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
            if child.Name == "ErrorPrompt" and child:FindFirstChild("MessageArea") and child.MessageArea:FindFirstChild("ErrorFrame") then
                game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
            end
        end)
        
        -- Store the connection for later cleanup
        _G.AutoRejoinConnection = autoRejoinConnection
    else
        -- Clean up auto rejoin
        if _G.AutoRejoinConnection then
            _G.AutoRejoinConnection:Disconnect()
            _G.AutoRejoinConnection = nil
        end
    end
end)

