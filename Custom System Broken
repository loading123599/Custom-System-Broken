-- Target functionality
local selectedTarget = nil

-- Function to get a player from a partial name
local function getPlayerFromString(name)
    name = name:lower()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player.Name:lower():sub(1, #name) == name then
            return player
        end
        if player.DisplayName and player.DisplayName:lower():sub(1, #name) == name then
            return player
        end
    end
    return nil
end

-- Update the target when the input changes
TargetBox_Input.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local targetName = TargetBox_Input.Text
        if targetName ~= "" then
            local player = getPlayerFromString(targetName)
            if player then
                selectedTarget = player
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Target Selected",
                    Text = "Target set to: " .. player.Name,
                    Duration = 3
                })
            else
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Target Error",
                    Text = "Player not found",
                    Duration = 3
                })
            end
        end
    end
end)

-- Target button functionality
KillTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Teleport to target and attempt to kill them
        local oldPosition = character.HumanoidRootPart.CFrame
        character.HumanoidRootPart.CFrame = targetCharacter.HumanoidRootPart.CFrame
        
        -- This is a basic implementation - actual kill functionality would depend on the game
        -- For demonstration purposes, we'll just create an explosion at the target
        local explosion = Instance.new("Explosion")
        explosion.Position = targetCharacter.HumanoidRootPart.Position
        explosion.BlastPressure = 500000
        explosion.BlastRadius = 5
        explosion.Parent = workspace
        
        -- Teleport back after a short delay
        task.wait(0.5)
        character.HumanoidRootPart.CFrame = oldPosition
    end
end)

TeleportTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        character.HumanoidRootPart.CFrame = targetCharacter.HumanoidRootPart.CFrame
    end
end)

FlingTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Store original properties
        local oldVelocity = character.HumanoidRootPart.Velocity
        local oldCFrame = character.HumanoidRootPart.CFrame
        
        -- Set up for flinging
        character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
        character.HumanoidRootPart.CanCollide = false
        
        -- Teleport to target and apply massive velocity
        character.HumanoidRootPart.CFrame = targetCharacter.HumanoidRootPart.CFrame
        character.HumanoidRootPart.Velocity = Vector3.new(500, 500, 500)
        
        -- Reset after a short delay
        task.wait(0.5)
        character.HumanoidRootPart.Velocity = oldVelocity
        character.HumanoidRootPart.CFrame = oldCFrame
        character.HumanoidRootPart.CanCollide = true
    end
end)

ViewTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local camera = workspace.CurrentCamera
    local targetCharacter = selectedTarget.Character
    
    if targetCharacter then
        camera.CameraSubject = targetCharacter.Humanoid
    end
end)

UnviewTarget_Button.MouseButton1Click:Connect(function()
    local camera = workspace.CurrentCamera
    local character = game.Players.LocalPlayer.Character
    
    if character and character:FindFirstChildOfClass("Humanoid") then
        camera.CameraSubject = character.Humanoid
    end
end)

FocusTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local targetCharacter = selectedTarget.Character
    
    if targetCharacter then
        -- Create a highlight effect around the target
        local highlight = Instance.new("Highlight")
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        highlight.Parent = targetCharacter
        
        -- Remove the highlight after 5 seconds
        task.delay(5, function()
            if highlight and highlight.Parent then
                highlight:Destroy()
            end
        end)
    end
end)

BenxTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Start a loop to follow the target
        local benxConnection
        benxConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if character and character:FindFirstChild("HumanoidRootPart") and 
               targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = targetCharacter.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.5)
            else
                benxConnection:Disconnect()
            end
        end)
        
        -- Stop after 10 seconds
        task.delay(10, function()
            if benxConnection then
                benxConnection:Disconnect()
            end
        end)
    end
end)

PushTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter and 
       character:FindFirstChild("HumanoidRootPart") and 
       targetCharacter:FindFirstChild("HumanoidRootPart") then
        
        -- Get direction from player to target
        local direction = (targetCharacter.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Unit
        
        -- Apply force to target
        if targetCharacter:FindFirstChildOfClass("Humanoid") then
            targetCharacter.HumanoidRootPart.Velocity = direction * 100
        end
    end
end)

WhitelistTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    -- Add target to whitelist (this is just a notification, actual implementation would depend on your whitelist system)
    game.StarterGui:SetCore("SendNotification", {
        Title = "Whitelist",
        Text = selectedTarget.Name .. " has been whitelisted",
        Duration = 3
    })
end)

HeadsitTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Start a loop to sit on target's head
        local sitConnection
        sitConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if character and character:FindFirstChild("HumanoidRootPart") and 
               targetCharacter and targetCharacter:FindFirstChild("Head") then
                character.HumanoidRootPart.CFrame = targetCharacter.Head.CFrame * CFrame.new(0, 0.5, 0)
            else
                sitConnection:Disconnect()
            end
        end)
        
        -- Stop after 10 seconds
        task.delay(10, function()
            if sitConnection then
                sitConnection:Disconnect()
            end
        end)
    end
end)

StandTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Start a loop to stand on target
        local standConnection
        standConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if character and character:FindFirstChild("HumanoidRootPart") and 
               targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = targetCharacter.HumanoidRootPart.CFrame * CFrame.new(0, 3, 0)
            else
                standConnection:Disconnect()
            end
        end)
        
        -- Stop after 10 seconds
        task.delay(10, function()
            if standConnection then
                standConnection:Disconnect()
            end
        end)
    end
end)

BackpackTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    -- Display target's backpack items
    local backpackItems = {}
    for _, item in pairs(selectedTarget.Backpack:GetChildren()) do
        if item:IsA("Tool") then
            table.insert(backpackItems, item.Name)
        end
    end
    
    local itemsText = table.concat(backpackItems, ", ")
    if itemsText == "" then
        itemsText = "No items found"
    end
    
    game.StarterGui:SetCore("SendNotification", {
        Title = selectedTarget.Name .. "'s Backpack",
        Text = itemsText,
        Duration = 5
    })
end)

DoggyTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Start a loop to follow behind target
        local doggyConnection
        doggyConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if character and character:FindFirstChild("HumanoidRootPart") and 
               targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart") then
                local targetCFrame = targetCharacter.HumanoidRootPart.CFrame
                character.HumanoidRootPart.CFrame = targetCFrame * CFrame.new(0, 0, 2) * CFrame.Angles(0, math.rad(180), 0)
            else
                doggyConnection:Disconnect()
            end
        end)
        
        -- Stop after 10 seconds
        task.delay(10, function()
            if doggyConnection then
                doggyConnection:Disconnect()
            end
        end)
    end
end)

DragTarget_Button.MouseButton1Click:Connect(function()
    if not selectedTarget then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Target Error",
            Text = "No target selected",
            Duration = 3
        })
        return
    end
    
    local character = game.Players.LocalPlayer.Character
    local targetCharacter = selectedTarget.Character
    
    if character and targetCharacter then
        -- Create a weld to drag the target
        local weld = Instance.new("Weld")
        weld.Part0 = character.HumanoidRootPart
        weld.Part1 = targetCharacter.HumanoidRootPart
        weld.C0 = CFrame.new(0, 0, -3)
        weld.Parent = character.HumanoidRootPart
        
        -- Remove the weld after 10 seconds
        task.delay(10, function()
            if weld and weld.Parent then
                weld:Destroy()
            end
        end)
    end
end)

-- Initialize the GUI with the Universal tab visible (already done in previous code)
-- Universal_Section.Visible = true

-- Final setup and cleanup
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.RightControl then
        Background.Visible = not Background.Visible
    end
end)

-- Add a cleanup function to ensure everything is properly removed when the script is stopped
local function cleanup()
    -- Stop any running connections
    for _, connection in pairs(getconnections(game:GetService("RunService").Heartbeat)) do
        connection:Disconnect()
    end
    
    -- Remove the GUI
    if Background and Background.Parent then
        Background:Destroy()
    end
end

-- Set up cleanup on script termination
game.Players.LocalPlayer.CharacterRemoving:Connect(cleanup)
