-- Add a search function
local Search_Input = Instance.new("TextBox")
Search_Input.Name = "Search_Input"
Search_Input.Parent = Background
Search_Input.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Search_Input.BackgroundTransparency = 0.300
Search_Input.BorderColor3 = Color3.fromRGB(255, 255, 255)
Search_Input.Position = UDim2.new(0.5, -75, 0, 5)
Search_Input.Size = UDim2.new(0, 150, 0, 25)
Search_Input.Font = Enum.Font.Gotham
Search_Input.PlaceholderColor3 = Color3.fromRGB(178, 178, 178)
Search_Input.PlaceholderText = "Search..."
Search_Input.Text = ""
Search_Input.TextColor3 = Color3.fromRGB(255, 255, 255)
Search_Input.TextSize = 14.000
Search_Input.TextWrapped = true

local function searchFunctions(query)
    query = query:lower()
    
    -- Make all sections visible for searching
    Universal_Section.Visible = true
    Target_Section.Visible = true
    Animations_Section.Visible = true
    Misc_Section.Visible = true
    
    -- Track if we found any matches
    local foundMatch = false
    
    -- Search through all buttons
    for _, obj in pairs(Background:GetDescendants()) do
        if obj:IsA("TextButton") and obj.Name:find("Button") then
            local buttonText = obj.Text:lower()
            
            if buttonText:find(query) then
                -- Highlight the button
                obj.BorderColor3 = Color3.fromRGB(255, 255, 0)
                obj.BorderSizePixel = 2
                
                -- Make sure the section containing this button is visible
                local parent = obj.Parent
                while parent ~= Background and parent ~= nil do
                    parent.Visible = true
                    parent = parent.Parent
                end
                
                foundMatch = true
            else
                -- Reset button appearance
                obj.BorderColor3 = Color3.fromRGB(0, 0, 0)
                obj.BorderSizePixel = 0
            end
        end
    end
    
    if not foundMatch and query ~= "" then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Search",
            Text = "No matches found for: " .. query,
            Duration = 3
        })
    end
end

Search_Input.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        searchFunctions(Search_Input.Text)
    end
end)

-- Final notification that everything is loaded
game.StarterGui:SetCore("SendNotification", {
    Title = "Poison's Hub v" .. version,
    Text = "Fully loaded! Press RightControl to toggle GUI.",
    Duration = 5
})

-- Return the main GUI object for external access
return Background
