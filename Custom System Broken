PlayerList_Title.TextSize = 18.000

local PlayerList_ScrollingFrame = Instance.new("ScrollingFrame")
PlayerList_ScrollingFrame.Name = "PlayerList_ScrollingFrame"
PlayerList_ScrollingFrame.Parent = PlayerList_Frame
PlayerList_ScrollingFrame.Active = true
PlayerList_ScrollingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
PlayerList_ScrollingFrame.BackgroundTransparency = 0.8
PlayerList_ScrollingFrame.BorderColor3 = Color3.fromRGB(255, 255, 255)
PlayerList_ScrollingFrame.BorderSizePixel = 1
PlayerList_ScrollingFrame.Position = UDim2.new(0, 0, 0, 30)
PlayerList_ScrollingFrame.Size = UDim2.new(1, 0, 1, -30)
PlayerList_ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
PlayerList_ScrollingFrame.ScrollBarThickness = 5

local PlayerList_UIListLayout = Instance.new("UIListLayout")
PlayerList_UIListLayout.Name = "PlayerList_UIListLayout"
PlayerList_UIListLayout.Parent = PlayerList_ScrollingFrame
PlayerList_UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
PlayerList_UIListLayout.Padding = UDim.new(0, 2)

-- Function to update the player list
local function updatePlayerList()
    -- Clear existing buttons
    for _, child in pairs(PlayerList_ScrollingFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    -- Add a button for each player
    local players = game.Players:GetPlayers()
    for i, player in ipairs(players) do
        local playerButton = Instance.new("TextButton")
        playerButton.Name = player.Name .. "_Button"
        playerButton.Parent = PlayerList_ScrollingFrame
        playerButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        playerButton.BackgroundTransparency = 0.5
        playerButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
        playerButton.BorderSizePixel = 1
        playerButton.Size = UDim2.new(1, -10, 0, 25)
        playerButton.Font = Enum.Font.Gotham
        playerButton.Text = player.Name
        playerButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        playerButton.TextSize = 14.000
        playerButton.TextXAlignment = Enum.TextXAlignment.Left
        
        -- Add click handler to select the player
        playerButton.MouseButton1Click:Connect(function()
            selectedTarget = player
            TargetBox_Input.Text = player.Name
            game.StarterGui:SetCore("SendNotification", {
                Title = "Target Selected",
                Text = "Target set to: " .. player.Name,
                Duration = 3
            })
        end)
    end
    
    -- Update canvas size
    PlayerList_ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, #players * 27) -- 25 + 2 padding
end

-- Toggle player list button
local PlayerList_Button = Instance.new("TextButton")
PlayerList_Button.Name = "PlayerList_Button"
PlayerList_Button.Parent = Background
PlayerList_Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
PlayerList_Button.BackgroundTransparency = 0.500
PlayerList_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
PlayerList_Button.BorderSizePixel = 0
PlayerList_Button.Position = UDim2.new(1, -30, 0, 5)
PlayerList_Button.Size = UDim2.new(0, 25, 0, 25)
PlayerList_Button.Font = Enum.Font.Oswald
PlayerList_Button.Text = "P"
PlayerList_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
PlayerList_Button.TextScaled = true
PlayerList_Button.TextSize = 14.000
PlayerList_Button.TextWrapped = true

PlayerList_Button.MouseButton1Click:Connect(function()
    PlayerList_Frame.Visible = not PlayerList_Frame.Visible
    if PlayerList_Frame.Visible then
        updatePlayerList()
    end
end)

-- Update player list when players join or leave
game.Players.PlayerAdded:Connect(function()
    if PlayerList_Frame.Visible then
        updatePlayerList()
    end
end)

game.Players.PlayerRemoving:Connect(function()
    if PlayerList_Frame.Visible then
        updatePlayerList()
    end
end)

