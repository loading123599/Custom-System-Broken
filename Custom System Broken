ESP_Toggle.Name = "ESP_Toggle"
ESP_Toggle.Parent = ESP_Frame
ESP_Toggle.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
ESP_Toggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
ESP_Toggle.BorderSizePixel = 1
ESP_Toggle.Position = UDim2.new(0.8, 0, 0, 0)
ESP_Toggle.Size = UDim2.new(0, 25, 0, 25)
ESP_Toggle.Font = Enum.Font.Gotham
ESP_Toggle.Text = ""
ESP_Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
ESP_Toggle.TextSize = 14.000

local espEnabled = false
local espObjects = {}

local function createESP(player)
    if player == game.Players.LocalPlayer then return end
    
    local highlight = Instance.new("Highlight")
    highlight.FillColor = Color3.fromRGB(255, 0, 0)
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Adornee = player.Character
    highlight.Parent = player.Character
    
    -- Create a billboard GUI for the name
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Name = "ESP_Name"
    billboardGui.Adornee = player.Character:WaitForChild("Head")
    billboardGui.Size = UDim2.new(0, 100, 0, 30)
    billboardGui.StudsOffset = Vector3.new(0, 2, 0)
    billboardGui.AlwaysOnTop = true
    billboardGui.Parent = player.Character.Head
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Name = "NameLabel"
    nameLabel.BackgroundTransparency = 1
    nameLabel.Size = UDim2.new(1, 0, 1, 0)
    nameLabel.Text = player.Name
    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    nameLabel.TextStrokeTransparency = 0
    nameLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 14
    nameLabel.Parent = billboardGui
    
    espObjects[player.Name] = {highlight = highlight, billboardGui = billboardGui}
    
    -- Update ESP when character changes
    player.CharacterAdded:Connect(function(character)
        if espEnabled and espObjects[player.Name] then
            -- Remove old ESP
            if espObjects[player.Name].highlight then
                espObjects[player.Name].highlight:Destroy()
            end
            if espObjects[player.Name].billboardGui then
                espObjects[player.Name].billboardGui:Destroy()
            end
            
            -- Create new ESP
            task.wait(1) -- Wait for character to load
            
            local newHighlight = Instance.new("Highlight")
            newHighlight.FillColor = Color3.fromRGB(255, 0, 0)
            newHighlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            newHighlight.FillTransparency = 0.5
            newHighlight.OutlineTransparency = 0
            newHighlight.Adornee = character
            newHighlight.Parent = character
            
            local newBillboardGui = Instance.new("BillboardGui")
            newBillboardGui.Name = "ESP_Name"
            newBillboardGui.Adornee = character:WaitForChild("Head")
            newBillboardGui.Size = UDim2.new(0, 100, 0, 30)
            newBillboardGui.StudsOffset = Vector3.new(0, 2, 0)
            newBillboardGui.AlwaysOnTop = true
            newBillboardGui.Parent = character.Head
            
            local newNameLabel = Instance.new("TextLabel")
            newNameLabel.Name = "NameLabel"
            newNameLabel.BackgroundTransparency = 1
            newNameLabel.Size = UDim2.new(1, 0, 1, 0)
            newNameLabel.Text = player.Name
            newNameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            newNameLabel.TextStrokeTransparency = 0
            newNameLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
            newNameLabel.Font = Enum.Font.GothamBold
            newNameLabel.TextSize = 14
            newNameLabel.Parent = newBillboardGui
            
            espObjects[player.Name] = {highlight = newHighlight, billboardGui = newBillboardGui}
        end
    end)
end

local function enableESP()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character then
            createESP(player)
        end
    end
    
    -- Connect to PlayerAdded event to create ESP for new players
    game.Players.PlayerAdded:Connect(function(player)
        if espEnabled then
            player.CharacterAdded:Wait() -- Wait for character to load
            createESP(player)
        end
    end)
end

local function disableESP()
    for playerName, objects in pairs(espObjects) do
        if objects.highlight then
            objects.highlight:Destroy()
        end
        if objects.billboardGui then
            objects.billboardGui:Destroy()
        end
    end
    
    espObjects = {}
end

ESP_Toggle.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    ESP_Toggle.BackgroundColor3 = espEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    
    if espEnabled then
        enableESP()
    else
        disableESP()
    end
end)

-- Settings button
local Settings_Button = Instance.new("TextButton")
Settings_Button.Name = "Settings_Button"
Settings_Button.Parent = Background
Settings_Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Settings_Button.BackgroundTransparency = 0.500
Settings_Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
Settings_Button.BorderSizePixel = 0
Settings_Button.Position = UDim2.new(0, 5, 0, 5)
Settings_Button.Size = UDim2.new(0, 25, 0, 25)
Settings_Button.Font = Enum.Font.Oswald
Settings_Button.Text = "⚙️"
Settings_Button.TextColor3 = Color3.fromRGB(0, 0, 0)
Settings_Button.TextScaled = true
Settings_Button.TextSize = 14.000
Settings_Button.TextWrapped = true

Settings_Button.MouseButton1Click:Connect(function()
    Settings_Frame.Visible = not Settings_Frame.Visible
end)

-- Add keybind system
local Keybinds = {
    ToggleGUI = Enum.KeyCode.RightControl,
    TogglePlayerList = Enum.KeyCode.P,
    ToggleSettings = Enum.KeyCode.O,
    KillTarget = Enum.KeyCode.K,
    TeleportToTarget = Enum.KeyCode.T
}

-- Function to handle keybinds
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Keybinds.ToggleGUI then
        Background.Visible = not Background.Visible
    elseif input.KeyCode == Keybinds.TogglePlayerList then
        PlayerList_Frame.Visible = not PlayerList_Frame.Visible
        if PlayerList_Frame.Visible then
            updatePlayerList()
        end
    elseif input.KeyCode == Keybinds.ToggleSettings then
        Settings_Frame.Visible = not Settings_Frame.Visible
    elseif input.KeyCode == Keybinds.KillTarget and selectedTarget then
        KillTarget_Button.MouseButton1Click:Fire()
    elseif input.KeyCode == Keybinds.TeleportToTarget and selectedTarget then
        TeleportTarget_Button.MouseButton1Click:Fire()
    end
end)

-- Final notification
game.StarterGui:SetCore("SendNotification", {
    Title = "Poison's Hub",
    Text = "All features loaded successfully!",
    Duration = 5
})

-- Add a version checker
local function checkForUpdates()
    local success, result = pcall(function()
        return game:HttpGet("https://raw.githubusercontent.com/PoisonHub/version.txt")
    end)
    
    if success then
        local latestVersion = tonumber(result)
        if latestVersion and latestVersion > version then
            game.StarterGui:SetCore("SendNotification", {
                Title = "Update Available",
                Text = "Version " .. latestVersion .. " is available!",
                Duration = 10
            })
        end
    end
end

-- Run the update checker
spawn(checkForUpdates)

-- Add a simple analytics system
local function sendAnalytics()
    pcall(function()
        local analyticsData = {
            username = game.Players.LocalPlayer.Name,
            placeId = game.PlaceId,
            version = version
        }
        
        -- This is just a placeholder URL, replace with your actual analytics endpoint
        game:HttpGet("https://poisonhub-analytics.com/track?data=" .. game:GetService("HttpService"):JSONEncode(analyticsData))
    end)
end

-- Run analytics in a separate thread
spawn(sendAnalytics)

-- Add a simple theme system
local themes = {
    Default = {
        Background = Color3.fromRGB(0, 0, 0),
        Text = Color3.fromRGB(255, 255, 255),
        Button = Color3.fromRGB(255, 255, 255),
        ButtonText = Color3.fromRGB(0, 0, 0),
        Border = Color3.fromRGB(255, 255, 255)
    },
    Dark = {
        Background = Color3.fromRGB(25, 25, 25),
        Text = Color3.fromRGB(200, 200, 200),
        Button = Color3.fromRGB(50, 50, 50),
        ButtonText = Color3.fromRGB(200, 200, 200),
        Border = Color3.fromRGB(100, 100, 100)
    },
    Light = {
        Background = Color3.fromRGB(200, 200, 200),
        Text = Color3.fromRGB(25, 25, 25),
        Button = Color3.fromRGB(150, 150, 150),
        ButtonText = Color3.fromRGB(25, 25, 25),
        Border = Color3.fromRGB(100, 100, 100)
    }
}

-- Function to apply a theme
local function applyTheme(themeName)
    local theme = themes[themeName]
    if not theme then return end
    
    -- Apply to main background
    Background.BackgroundColor3 = theme.Background
    
    -- Apply to all buttons
    for _, obj in pairs(Background:GetDescendants()) do
        if obj:IsA("TextButton") then
            if obj.BackgroundTransparency < 1 then
                obj.BackgroundColor3 = theme.Button
            end
            obj.TextColor3 = theme.ButtonText
        elseif obj:IsA("TextLabel") then
            obj.TextColor3 = theme.Text
        elseif obj:IsA("Frame") and obj.BorderSizePixel > 0 then
            obj.BorderColor3 = theme.Border
        end
    end
    
    -- Apply to player list and settings frames
    PlayerList_Frame.BackgroundColor3 = theme.Background
    PlayerList_Frame.BorderColor3 = theme.Border
    Settings_Frame.BackgroundColor3 = theme.Background
    Settings_Frame.BorderColor3 = theme.Border
    
    -- Save the current theme
    _G.CurrentTheme = themeName
end

-- Add theme selector to settings
local Theme_Frame = Instance.new("Frame")
Theme_Frame.Name = "Theme_Frame"
Theme_Frame.Parent = Settings_Frame
Theme_Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
